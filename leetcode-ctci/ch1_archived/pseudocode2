Class LinkedList: 
    Class Node: 
        def __init__(self, value, next):
            self.value=value
            self.next=None
    
    def __init__(self):
        self.head=None
        self.length=0
    
    def push(self, value):
        n=self.Node(value, None)
        if self.head==None: 
            self.head=n
        else: 
            h=self.head
            while(h.next!=None):
                h=h.next
            h.next=n

    
    def delete(self, value):
        found=False
        h=self.head
        if h.value==value: 
            self.head=self.head.next
        else: 
            while(h.next!=None):
                if h.next.value==value: 
                    h.next=h.next.next
                    found=True
                    return value
            if found==False: 
                return None

def main():
    l=LinkedList()
    l.push(3)
    l.push(4)
    l.push(5)
    
            
                
            




